<launch>
  <arg name="robotNamespace" default="ben"/>
  <arg name="remote" default="ben"/>
  <arg name="namespace" default="operator"/>

  <node pkg="rosservice" type="rosservice" name="get_pano1" args="call /$(arg namespace)/udp_bridge/remote_subscribe $(arg remote) /$(arg robotNamespace)/sensors/cameras/pano_1/thumbnail/image/compressed /$(arg robotNamespace)/sensors/cameras/pano_1/image_raw/compressed 1 1.0"/>
  <node pkg="rosservice" type="rosservice" name="get_pano2" args="call /$(arg namespace)/udp_bridge/remote_subscribe $(arg remote) /$(arg robotNamespace)/sensors/cameras/pano_2/thumbnail/image/compressed /$(arg robotNamespace)/sensors/cameras/pano_2/image_raw/compressed 1 1.0"/>
  <node pkg="rosservice" type="rosservice" name="get_pano5" args="call /$(arg namespace)/udp_bridge/remote_subscribe $(arg remote) /$(arg robotNamespace)/sensors/cameras/pano_5/thumbnail/image/compressed /$(arg robotNamespace)/sensors/cameras/pano_1/image_raw/compressed 1 1.0"/>

</launch>
